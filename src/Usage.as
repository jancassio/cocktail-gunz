package  {	import user.User;	import user.gunz.UserBullet;	import user.gunz.UserTrigger;		import flash.display.Sprite;			/**	 * Usage example of a Trigger / Finger / Bullet (event) flow.	 * @author nybras | nybras@codeine.it	 */	public class Usage extends Sprite	{		private var _user : User;				public function Usage ()		{			// new user			_user = new User ();						// listening for update			_user.listen.update ( update, [ "testing params", 1, 2, 3 ] );						// updating user (this fires the change event)			_user.update( "nybras", "m", 24 );					}				public function update ( event : UserBullet ) : void		{			trace ( "--------------------" );			trace ( event +":"+ event.type );			trace ( "--------------------" );			trace ( event.name );			trace ( event.age );			trace ( event.gender );			trace ( event.params );		}	}}